<?xml version="1.0" encoding="utf-8"?>



<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0E0E0E"
    android:foregroundTint="#100F0F"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-medium"
        android:text="App Name"
        android:textColor="#F44336"
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <Button
        android:id="@+id/watch"
        android:layout_width="65dp"
        android:layout_height="70dp"
        android:layout_marginStart="64dp"
        android:backgroundTint="#4D7EDD"
        android:foregroundTint="#953535"
        android:text="🎬"
        android:textColor="#913636"
        android:textSize="26sp"
        app:backgroundTint="@color/white"
        app:iconTint="#C56485"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/channels"
        app:layout_constraintHorizontal_bias="0.069"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.239" />

    <Button
        android:id="@+id/channels"
        android:layout_width="65dp"
        android:layout_height="70dp"
        android:backgroundTint="#4D7EDD"
        android:foregroundTint="#953535"
        android:text="💭"
        android:textColor="#913636"
        android:textSize="26sp"
        app:backgroundTint="@color/white"
        app:iconTint="#C56485"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.786"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.239" />

    <Button
        android:id="@+id/project"
        android:layout_width="65dp"
        android:layout_height="70dp"
        android:layout_marginStart="72dp"
        android:backgroundTint="#4D7EDD"
        android:foregroundTint="#953535"
        android:text="🗃️"
        android:textColor="#913636"
        android:textColorHint="#ACA52F"
        android:textSize="26sp"
        app:backgroundTint="@color/white"
        app:iconTint="#C56485"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/listen"
        app:layout_constraintHorizontal_bias="0.014"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.475" />

    <Button
        android:id="@+id/listen"
        android:layout_width="65dp"
        android:layout_height="70dp"
        android:backgroundTint="#4D7EDD"
        android:foregroundTint="#953535"
        android:text="🎶"
        android:textColor="#913636"
        android:textSize="26sp"
        app:backgroundTint="@color/white"
        app:iconTint="#C56485"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.786"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.475" />

    <Button
        android:id="@+id/team"
        android:layout_width="65dp"
        android:layout_height="70dp"
        android:layout_marginStart="72dp"
        android:backgroundTint="#4D7EDD"
        android:foregroundTint="#953535"
        android:text="🤝"
        android:textColor="#913636"
        android:textSize="26sp"
        app:backgroundTint="@color/white"
        app:iconTint="#C56485"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/fact"
        app:layout_constraintHorizontal_bias="0.014"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.715" />

    <Button
        android:id="@+id/fact"
        android:layout_width="65dp"
        android:layout_height="70dp"
        android:backgroundTint="#4D7EDD"
        android:foregroundTint="#953535"
        android:text="📜"
        android:textColor="#913636"
        android:textSize="26sp"
        app:backgroundTint="@color/white"
        app:iconTint="#C56485"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.786"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.715" />

</androidx.constraintlayout.widget.ConstraintLayout>